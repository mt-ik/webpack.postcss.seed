    "size-limit": "^0.17.0",
    "stryker": "^0.20.1",
    "stryker-api": "^0.14.0",
    "stryker-babel-transpiler": "^0.3.4",
    "stryker-html-reporter": "^0.12.4",
    "stryker-javascript-mutator": "^0.4.3",
    "stryker-jest-runner": "^0.5.1",
    "stryker-webpack-transpiler": "^0.2.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
