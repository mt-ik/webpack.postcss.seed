{
  "name": "mt.ik.webpack.postcss",
  "version": "1.0.0",
  "description": "postcss studing",
  "sideEffects": true,
  "private": false,
  "size-limit": [
    {
      "path": "index.js"
    }
  ],
  "scripts": {
    "test:start": "webpack-dev-server --env development ./webpack/webpack.test.js",
    "test:dev": "nodemon --watch ./webpack/webpack.test.js --exec \"webpack-dev-server --env development --config ./webpack/webpack.test.js\"",
    "test:build": "webpack --env production --config ./webpack/webpack.test.js",
    "start": "webpack-dev-server --env development ./webpack/webpack.common.js",
    "dev": "nodemon --watch ./webpack/webpack.common.js --exec \"webpack-dev-server --env development --config ./webpack/webpack.common.js\"",
    "build": "webpack --env production --config ./webpack/webpack.common.js",
    "watch": "webpack --watch",
    "dash": "webpack-dashboard --node ./src/index.js",
    "stats": "webpack --env --config ./webpack/webpack.prod.js --display-reasons --profile --progress --json > stats.json",
    "build:i18n": "webpack --config ./webpack/webpack.i18n.js",
    "prepublishOnly": "pkg-ok",
    "test:karma": "karma start",
    "test:karma:watch": "karma start --auto-watch --no-single-run",
    "test:jest:watch": "jest --watch",
    "test:jest": "jest",
    "size": "size-limit",
    "deploy": "gh-pages -d build",
    "lint:es": "eslint . --fix",
    "lint:css": "stylelint --fix '**/*.css'",
    "lint:ts": "tslint --fix 'src/**/*.ts'",
    "format": "prettier --write '**/*.{js,css,md}'",
    "test": "coverage",
    "after_success": "- 'bash <(curl -s https://codecov.io/bash)'",
    "danger": "danger"
  },
  "author": "xuguangyu <xujian1@guazi.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
