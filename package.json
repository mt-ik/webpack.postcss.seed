    "lint-staged": "^7.0.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "optimize-js-plugin": "^0.0.4",
    "pkg-ok": "^2.1.0",
    "postcss": "^6.0.21",
