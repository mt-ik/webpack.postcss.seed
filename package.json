{
  "name": "mt.ik.webpack.postcss",
  "version": "1.0.0",
  "description": "postcss studing",
  "sideEffects": true,
  "private": false,
  "scripts": {
    "default": "webpack-dev-server --open",
    "start": "webpack-dev-server --env --open --config ./webpack/webpack.dev.js",
    "dev": "nodemon --watch ./webpack/webpack.dev.js --exec \"webpack-dev-server --env --display-used-exports --config ./webpack/webpack.dev.js\"",
    "build": "webpack --env --config ./webpack/webpack.prod.js",
    "watch": "webpack --watch",
    "dash": "webpack-dashboard --node ./src/index.js",
    "stats": "webpack --env --config ./webpack/webpack.prod.js --display-reasons --profile --progress --json > stats.json",
    "build:i18n": "webpack --config ./webpack/webpack.i18n.js",
    "test:mocha": "mocha --watch ./src/tests",
    "test:mocha:watch": "webpack-dev-server --hot --config ./webpack/webpack.mocha.js",
    "test:karma": "karma start",
    "test:karma:watch": "karma start --auto-watch --no-single-run",
    "test:jest:watch": "jest --watch",
    "test:jest": "jest",
    "deploy": "gh-pages -d build",
    "lint:es": "eslint . --fix",
    "lint:css": "stylelint --fix '**/*.css'",
    "lint:ts": "tslint --fix 'src/**/*.ts'",
    "format": "prettier --write '**/*.{js,css,md}'",
    "test": "coverage", 
    "after_success": "- 'bash <(curl -s https://codecov.io/bash)'"
  },
  "author": "xuguangyu <xujian1@guazi.com>",
  "license": "ISC",
