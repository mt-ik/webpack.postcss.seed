    "file-loader": "^1.1.11",
    "git-revision-webpack-plugin": "^2.5.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "i18n-webpack-plugin": "^1.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "less": "^3.0.1",
